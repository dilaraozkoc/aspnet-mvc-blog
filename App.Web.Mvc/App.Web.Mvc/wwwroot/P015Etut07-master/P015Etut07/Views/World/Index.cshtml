@model List<Product>


@*@await Component.InvokeAsync("Archives")*@


@foreach(var item in Model)
{
    @await Component.InvokeAsync("ProductItem", new{title = item.Title, starCount = item.StarCount,  oldPrice = item.OldPrice,  price = item.Price,  imageUrl = item.ImageUrl ,  isSale = item.IsSale})

}

